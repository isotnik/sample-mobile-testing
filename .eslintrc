{
  "plugins": [
    "wdio"
  ],
  "extends": [
    "plugin:wdio/recommended",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "mocha": true,
    "node": true
  },
  "globals": {
    "$": true,
    "$$": true,
    "driver": true,
    "config": true,
    "__ENV": true
  },
  "rules": {
    "no-unused-vars": "warn"
  }
}